{
    "name": "emojify-lyrics",
    "version": "1.5.2",
    "description": "Making the world a :slight_smile: place",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "license": "MIT",
    "author": "Madeline Miller",
    "dependencies": {
        "emojilib": "^3.0.6",
        "unicode-emoji-json": "^0.3.1"
    },
    "scripts": {
        "prepublish": "node scripts/generateEmojiMap.js && tsc",
        "build": "tsc",
        "clean": "rm -rf dist/",
        "test": "jest",
        "lint": "eslint --config ./.eslintrc.js --ext .ts,.js src/ scripts/",
        "lint:fix": "eslint --config ./.eslintrc.js --fix --ext .ts,.js src/ scripts/",
        "postinstall": "husky install",
        "prepare": "husky install",
        "precommit": "lint-staged"
    },
    "devDependencies": {
        "@types/jest": "^27.4.1",
        "@typescript-eslint/eslint-plugin": "^5.15.0",
        "@typescript-eslint/parser": "^5.15.0",
        "eslint": "^8.11.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-prettier": "^0.0.0-empty",
        "fs-extra": "^10.0.1",
        "husky": "^7.0.4",
        "jest": "^27.5.1",
        "lint-staged": "^12.3.5",
        "prettier": "^2.5.1",
        "ts-jest": "^27.1.3",
        "typescript": "^4.6.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
